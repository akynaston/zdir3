BEGIN { FS = ":: "; OFS = ": " }
{
    if (NF == 2) {
        # Decode base64-encoded value
        cmd = "echo \"" $2 "\" | base64 -d"
        cmd | getline decoded
        close(cmd)
        print $1, decoded
    } else {
        print
    }
}
' "$INPUT_FILE" > "$OUTPUT_FILE"
